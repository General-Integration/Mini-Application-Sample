{"ast":null,"code":"import \"core-js/modules/web.dom-exception.stack.js\";\nexport default {\n  created() {\n    // set bar title\n    this.$bridge.callHandler(\"setBarTitle\", {\n      title: \"Google Sign In Page\"\n    });\n  },\n  methods: {\n    decodeJwtResponse(token) {\n      let base64Url = token.split(\".\")[1];\n      let base64 = base64Url.replace(/-/g, \"+\").replace(/_/g, \"/\");\n      let jsonPayload = decodeURIComponent(atob(base64).split(\"\").map(function (c) {\n        return \"%\" + (\"00\" + c.charCodeAt(0).toString(16)).slice(-2);\n      }).join(\"\"));\n      return JSON.parse(jsonPayload);\n    },\n    handleCredentialResponse(response) {\n      const responsePayload = this.decodeJwtResponse(response.credential);\n      document.getElementById('name').innerHTML = responsePayload.name;\n      document.getElementById('email').innerHTML = responsePayload.email;\n    }\n  },\n  mounted() {\n    try {\n      google.accounts.id.initialize({\n        client_id: \"778205346325-am0vd834qdodnpqfn8a56d1b2o6bvddq.apps.googleusercontent.com\",\n        callback: this.handleCredentialResponse\n      });\n      google.accounts.id.renderButton(document.getElementById(\"buttonDiv\"), {\n        theme: \"outline\",\n        size: \"large\"\n      } // customization attributes\n      );\n\n      google?.accounts.id.prompt(); // also display the One Tap dialog\n    } catch (error) {\n      /** error */\n    }\n  }\n};","map":{"version":3,"names":["created","$bridge","callHandler","title","methods","decodeJwtResponse","token","base64Url","split","base64","replace","jsonPayload","decodeURIComponent","atob","map","c","charCodeAt","toString","slice","join","JSON","parse","handleCredentialResponse","response","responsePayload","credential","document","getElementById","innerHTML","name","email","mounted","google","accounts","id","initialize","client_id","callback","renderButton","theme","size","prompt","error"],"sources":["/Users/chhuonn.sophorn/Desktop/ABA-09-01-2023/Mini-Application-Sample/src/pages/GoogleSign.vue"],"sourcesContent":["<script>\nexport default {\n  created() {\n    // set bar title\n    this.$bridge.callHandler(\"setBarTitle\", { title: \"Google Sign In Page\" });\n  },\n  methods: {\n    decodeJwtResponse (token) {\n      let base64Url = token.split(\".\")[1];\n      let base64 = base64Url.replace(/-/g, \"+\").replace(/_/g, \"/\");\n      let jsonPayload = decodeURIComponent(\n        atob(base64).split(\"\").map(function (c) { \n          return \"%\" + (\"00\" + c.charCodeAt(0).toString(16)).slice(-2); \n        }).join(\"\")\n      );\n      return JSON.parse(jsonPayload);\n    },\n    handleCredentialResponse (response) {\n      const responsePayload = this.decodeJwtResponse(response.credential);\n      document.getElementById('name').innerHTML = responsePayload.name\n      document.getElementById('email').innerHTML = responsePayload.email\n    }\n  },\n  mounted() {\n    try{\n      google.accounts.id.initialize({\n        client_id: \"778205346325-am0vd834qdodnpqfn8a56d1b2o6bvddq.apps.googleusercontent.com\",\n        callback: this.handleCredentialResponse\n      });\n      google.accounts.id.renderButton(\n        document.getElementById(\"buttonDiv\"),\n        { theme: \"outline\", size: \"large\" }  // customization attributes\n      );\n      google?.accounts.id.prompt(); // also display the One Tap dialog\n    }catch(error){\n      /** error */\n    }\n  }\n};\n</script>\n<template>\n  <div class=\"wrapper\" style=\"margin-bottom: 60px\">\n    <h2 style=\"padding: 16px\">Google Sign In</h2>\n    <div class=\"description\" :style=\"{display: 'flex', 'flex-direction': 'column', 'justify-content': 'center', 'align-items': 'center'}\">\n      <div id=\"buttonDiv\">Google</div>\n      <table>\n        <thead>\n          <tr>\n            <td>Properties</td>\n            <td>Value</td>\n          </tr>\n        </thead>\n        <tbody>\n          <tr>\n            <td>Full Name</td>\n            <td id=\"name\"></td>\n          </tr>\n          <tr>\n            <td>Email</td>\n            <td id=\"email\"></td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </div>\n</template>\n<style lang=\"scss\">\n.wrapper {\n  .description {\n    padding: 16px;\n    display: flex;\n    justify-content: center;\n    .img {\n      width: 100%;\n      height: 400px;\n      object-fit: contain;\n    }\n  }\n  h2 {\n    padding: 16px 16px 0;\n  }\n}\nbody {\n  background: #fff;\n}\ntable{\n  width: 100%;\n  margin-top: 20px;\n  border: 1px solid gray;\n  border-collapse: collapse;\n}\ntable thead td{\n  width: 50%;\n  text-align: left;\n  padding: 10px 10px;\n  background-color: rgb(218, 222, 220);\n}\ntable tbody td{\n  width: 50%;\n  text-align: left;\n  padding: 5px 10px;\n  font-size: 14px;\n  border: 1px solid;\n}\n</style>"],"mappings":";AACA,eAAe;EACbA,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACC,OAAO,CAACC,WAAW,CAAC,aAAa,EAAE;MAAEC,KAAK,EAAE;IAAsB,CAAC,CAAC;EAC3E,CAAC;EACDC,OAAO,EAAE;IACPC,iBAAgBA,CAAGC,KAAK,EAAE;MACxB,IAAIC,SAAQ,GAAID,KAAK,CAACE,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MACnC,IAAIC,MAAK,GAAIF,SAAS,CAACG,OAAO,CAAC,IAAI,EAAE,GAAG,CAAC,CAACA,OAAO,CAAC,IAAI,EAAE,GAAG,CAAC;MAC5D,IAAIC,WAAU,GAAIC,kBAAkB,CAClCC,IAAI,CAACJ,MAAM,CAAC,CAACD,KAAK,CAAC,EAAE,CAAC,CAACM,GAAG,CAAC,UAAUC,CAAC,EAAE;QACtC,OAAO,GAAE,GAAI,CAAC,IAAG,GAAIA,CAAC,CAACC,UAAU,CAAC,CAAC,CAAC,CAACC,QAAQ,CAAC,EAAE,CAAC,EAAEC,KAAK,CAAC,CAAC,CAAC,CAAC;MAC9D,CAAC,CAAC,CAACC,IAAI,CAAC,EAAE,CACZ,CAAC;MACD,OAAOC,IAAI,CAACC,KAAK,CAACV,WAAW,CAAC;IAChC,CAAC;IACDW,wBAAuBA,CAAGC,QAAQ,EAAE;MAClC,MAAMC,eAAc,GAAI,IAAI,CAACnB,iBAAiB,CAACkB,QAAQ,CAACE,UAAU,CAAC;MACnEC,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,SAAQ,GAAIJ,eAAe,CAACK,IAAG;MAC/DH,QAAQ,CAACC,cAAc,CAAC,OAAO,CAAC,CAACC,SAAQ,GAAIJ,eAAe,CAACM,KAAI;IACnE;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAG;MACDC,MAAM,CAACC,QAAQ,CAACC,EAAE,CAACC,UAAU,CAAC;QAC5BC,SAAS,EAAE,0EAA0E;QACrFC,QAAQ,EAAE,IAAI,CAACf;MACjB,CAAC,CAAC;MACFU,MAAM,CAACC,QAAQ,CAACC,EAAE,CAACI,YAAY,CAC7