  this.$router.push(\"/upload\");\n          break;\n        case 3:\n          this.$router.push(\"/google-sign\");\n          break;\n        default:\n          break;\n      }\n    },\n    handleCloseApp() {\n      Dialog.confirm({\n        title: \"Leave Mini Application?\",\n        cancelButtonText: \"Cancel\",\n        confirmButtonText: \"Leave\",\n      })\n        .then(() => {\n          this.$bridge.callHandler(\"closeApp\").then(() => {});\n        })\n        .catch(() => {});\n    },\n  },\n};\n</script>\n\n<template>\n  <div>\n    <!-- <AppHeader /> -->\n    <div>\n      <router-view />\n    </div>\n    <van-tabbar v-model=\"active\" @change=\"handleChange\">\n      <van-tabbar-item @click=\"redirect(0)\" icon=\"wap-home-o\"\n        >Home</van-tabbar-item\n      >\n      <van-tabbar-item @click=\"redirect(1)\">\n        <span>Profile</span>\n        <template #icon=\"props\">\n          <img :src=\"props.active ? icon.active : icon.inactive\" alt=\"\"/>\n        </template>\n      </van-tabbar-item>\n      <van-tabbar-item @click=\"redirect(2)\" icon=\"idcard\"\n        >Upload</van-tabbar-item\n      >\n      <van-tabbar-item @click=\"redirect(3)\">\n        <div :style=\"{display: 'flex', 'flex-direction': 'column', 'justify-content': 'center', 'align-items': 'center'}\">\n          <img :style=\"{width: '20px'}\" src=\"https://static.vecteezy.com/system/resources/thumbnails/012/871/371/small/google-search-icon-google-product-illustration-free-png.png\" alt=\"\"/>\n          <p :style=\"{'margin-top': '5px', 'margin-bottom': '3px'}\">Google Sign In</p>\n        </div>\n      </van-tabbar-item>\n      <!-- <van-tabbar-item to=\"/payment\" icon=\"idcard\">Payment</van-tabbar-item> -->\n    </van-tabbar>\n    <!-- <AppFooter /> -->\n  </div>\n</template>\n<style>\n  .vc-switch{\n    bottom: 100px !important;\n  }\n</style>\n"],"mappings":";;;;;;;AACA;AACA,OAAOA,QAAO,MAAO,UAAU;AAG/B,IAAIA,QAAQ,CAAC;EAAEC,KAAK,EAAE;AAAQ,CAAC,CAAC;AAEhC,eAAe;EACbC,IAAI